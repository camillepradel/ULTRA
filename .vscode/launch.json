{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "inductive train and eval",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/script/run.py",
            "console": "integratedTerminal",
            "args": "-c ${workspaceFolder}/config/inductive/inference.yaml --dataset FB15k237Inductive --version v1 --epochs 10 --bpe null --gpus null --ckpt null",
            "env": {
                "CUDA_LAUNCH_BLOCKING": "1"
            }
        },
        {
            "name": "pretrain",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/script/pretrain.py",
            "console": "integratedTerminal",
            "args": "-c ${workspaceFolder}/config/transductive/pretrain_3g.yaml --gpus [0]"
        },
        {
            "name": "Run ULTRA",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/script/run.py",
            "console": "integratedTerminal",
            "args": "python script/run.py -c ${workspaceFolder}/config/transductive/inference.yaml --dataset CoDExSmall --epochs 0 --bpe null --gpus [0] --ckpt ${workspaceFolder}/ckpts/ultra_4g.pth"
        }
    ]
}